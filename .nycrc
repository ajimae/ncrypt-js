{
  "cache": false,
  "check-coverage": false,
  "extension": [
    ".ts"
  ],
  "require": [
    "ts-node/register"
  ],
  "include": [
    "**/*js",
    "**/*ts"
  ],
  "exclude": [
    "dist/**",
    "coverage/**",
    "node_modules/**",
    "**/*.d.ts",
    "**/*.test.ts"
  ],
  "sourceMap": true,
  "reporter": [
    "html",
    "text",
    "text-summary"
  ],
  "all": true,
  "instrument": true
}